<Boxel::CustomRadio::Container
  @groupDescription={{@groupDescription}}
  @disabled={{@disabled}}
  ...attributes
>
  {{#each @items as |item|}}
    {{yield
      (hash
        component=(component "boxel/custom-radio/item")
        data=item
        checked=(eq item.id @checkedId)
        disabled=@disabled
      )
    }}
  {{/each}}
</Boxel::CustomRadio::Container>