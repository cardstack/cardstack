<div class='card-wallet-badges' ...attributes>
  <div>
    <a href={{this.appStoreLink}} target='_blank' rel='noopener noreferrer'>
      <img
        src='/images/icons/download-on-the-app-store-badge.png'
        alt='Download on the App Store'
      />
    </a>
  </div>
  <div>
    <a href={{this.googlePlayLink}} target='_blank' rel='noopener noreferrer'>
      <img
        src='/images/icons/google-play-badge.png'
        class='card-pay-dashboard__wallet-badges-google-play-badge'
        alt='Get it on Google Play'
      />
    </a>
  </div>
</div>