<CardPay::MerchantCard
  @balance={{@safe.accumulatedSpendValue}}
  @usdBalance={{spend-to-usd @safe.accumulatedSpendValue}}
  @address={{@safe.address}}
  @managerCount={{@safe.owners.length}}
  @network={{network-display-info "layer2" "fullName"}}
