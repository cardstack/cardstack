<div class="funding-source-token-option" ...attributes>
  {{svg-jar @tokenIcon class="funding-source-token-option__icon" role="presentation" width="20" height="20"}}
  <div>
    <div class="funding-source-token-option__title">
      {{@tokenSymbol}}
    </div>
    <div class="funding-source-token-option__balance" data-test-funding-source-option={{@tokenSymbol}}>
      {{@balance}} {{@tokenSymbol}}.CPXD
    </div>
  </div>
  {{#if @selected}}
    {{svg-jar "checkmark"}}
  {{/if}}
</div>