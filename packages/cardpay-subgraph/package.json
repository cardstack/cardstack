{
  "name": "@cardstack/cardpay-subgraph",
  "version": "1.0.46",
  "license": "MIT",
  "scripts": {
    "codegen-localchain": "node ./etc/pre-codegen.js localchain && graph codegen && node ./etc/pre-tsc-build.js",
    "codegen-sokol": "node ./etc/pre-codegen.js poa-sokol && graph codegen && node ./etc/pre-tsc-build.js",
    "codegen-xdai": "node ./etc/pre-codegen.js gnosis && graph codegen && node ./etc/pre-tsc-build.js",
    "build": "graph build",
    "codegen-graft-sokol-green": "node ./etc/pre-codegen.js poa-sokol https://graph-staging-green.stack.cards/subgraphs/name/habdelra/cardpay-sokol && graph codegen && node ./etc/pre-tsc-build.js",
    "create-sokol-green": "graph create --node https://admin-graph-staging-green.stack.cards/ habdelra/cardpay-sokol",
    "remove-sokol-green": "graph remove --node https://admin-graph-staging-green.stack.cards/ habdelra/cardpay-sokol",
    "deploy-sokol-green": "ts-node ./lib/validate-subgraph-network.ts sokol && graph deploy --node https://admin-graph-staging-green.stack.cards/ --ipfs https://ipfs-graph-staging-green.stack.cards habdelra/cardpay-sokol",
    "codegen-graft-sokol-blue": "node ./etc/pre-codegen.js poa-sokol https://graph-staging-blue.stack.cards/subgraphs/name/habdelra/cardpay-sokol && graph codegen && node ./etc/pre-tsc-build.js",
    "create-sokol-blue": "graph create --node https://admin-graph-staging-blue.stack.cards/ habdelra/cardpay-sokol",
    "remove-sokol-blue": "graph remove --node https://admin-graph-staging-blue.stack.cards/ habdelra/cardpay-sokol",
    "deploy-sokol-blue": "ts-node ./lib/validate-subgraph-network.ts sokol && graph deploy --node https://admin-graph-staging-blue.stack.cards/ --ipfs https://ipfs-graph-staging-blue.stack.cards habdelra/cardpay-sokol",
    "codegen-graft-sokol-justin": "node ./etc/pre-codegen.js poa-sokol https://graph-justin.stack.cards/subgraphs/name/habdelra/cardpay-sokol && graph codegen && node ./etc/pre-tsc-build.js",
    "create-sokol-justin": "graph create --node https://admin-graph-justin.stack.cards/ habdelra/cardpay-sokol",
    "remove-sokol-justin": "graph remove --node https://admin-graph-justin.stack.cards/ habdelra/cardpay-sokol",
    "deploy-sokol-justin": "ts-node ./lib/validate-subgraph-network.ts sokol && graph deploy --node https://admin-graph-justin.stack.cards/ --ipfs https://ipfs-graph-justin.stack.cards habdelra/cardpay-sokol",
    "codegen-graft-sokol-hassan": "node ./etc/pre-codegen.js poa-sokol https://graph-hassan.stack.cards/subgraphs/name/habdelra/cardpay-sokol && graph codegen && node ./etc/pre-tsc-build.js",
    "create-sokol-hassan": "graph create --node https://admin-graph-hassan.stack.cards/ habdelra/cardpay-sokol",
    "remove-sokol-hassan": "graph remove --node https://admin-graph-hassan.stack.cards/ habdelra/cardpay-sokol",
    "deploy-sokol-hassan": "ts-node ./lib/validate-subgraph-network.ts sokol && graph deploy --node https://admin-graph-hassan.stack.cards/ --ipfs https://ipfs-graph-hassan.stack.cards habdelra/cardpay-sokol",
    "codegen-graft-xdai-hassan": "node ./etc/pre-codegen.js gnosis https://graph-hassan.stack.cards/subgraphs/name/habdelra/cardpay-xdai && graph codegen && node ./etc/pre-tsc-build.js",
    "create-xdai-hassan": "graph create --node https://admin-graph-hassan.stack.cards/ habdelra/cardpay-xdai",
    "remove-xdai-hassan": "graph remove --node https://admin-graph-hassan.stack.cards/ habdelra/cardpay-xdai",
    "deploy-xdai-hassan": "ts-node ./lib/validate-subgraph-network.ts gnosis && graph deploy --node https://admin-graph-hassan.stack.cards/ --ipfs https://ipfs-graph-hassan.stack.cards habdelra/cardpay-xdai",
    "codegen-graft-xdai-green": "node ./etc/pre-codegen.js gnosis https://graph-production-green.cardstack.com/subgraphs/name/habdelra/cardpay-xdai && graph codegen && node ./etc/pre-tsc-build.js",
    "create-xdai-green": "graph create --node https://admin-graph-production-green.cardstack.com/ habdelra/cardpay-xdai",
    "remove-xdai-green": "graph remove --node https://admin-graph-production-green.cardstack.com/ habdelra/cardpay-xdai",
    "deploy-xdai-green": "ts-node ./lib/validate-subgraph-network.ts gnosis && graph deploy --node https://admin-graph-production-green.cardstack.com/ --ipfs https://ipfs-graph-production-green.cardstack.com habdelra/cardpay-xdai",
    "codegen-graft-xdai-blue": "node ./etc/pre-codegen.js gnosis https://graph-production-blue.cardstack.com/subgraphs/name/habdelra/cardpay-xdai && graph codegen && node ./etc/pre-tsc-build.js",
    "create-xdai-blue": "graph create --node https://admin-graph-production-blue.cardstack.com/ habdelra/cardpay-xdai",
    "remove-xdai-blue": "graph remove --node https://admin-graph-production-blue.cardstack.com/ habdelra/cardpay-xdai",
    "deploy-xdai-blue": "ts-node ./lib/validate-subgraph-network.ts gnosis && graph deploy --node https://admin-graph-production-blue.cardstack.com/ --ipfs https://ipfs-graph-production-blue.cardstack.com habdelra/cardpay-xdai",
    "codegen-graft-local": "node ./etc/pre-codegen.js gnosis http://localhost:8000/subgraphs/name/habdelra/cardpay-xdai && graph codegen && node ./etc/pre-tsc-build.js",
    "create-local-xdai": "graph create --node http://localhost:8020/ habdelra/cardpay-xdai",
    "remove-local-xdai": "graph remove --node http://localhost:8020/ habdelra/cardpay-xdai",
    "deploy-local-xdai": "ts-node ./lib/validate-subgraph-network.ts gnosis && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 habdelra/cardpay-xdai",
    "sokol-green-status": "ts-node ./lib/sync-status.ts sokol-green",
    "sokol-blue-status": "ts-node ./lib/sync-status.ts sokol-blue",
    "xdai-green-status": "ts-node ./lib/sync-status.ts xdai-green",
    "xdai-blue-status": "ts-node ./lib/sync-status.ts xdai-blue",
    "remove-localchain": "graph remove --node http://localhost:8020 localchain",
    "create-localchain": "graph create --node http://localhost:8020 localchain",
    "deploy-localchain": "graph deploy localchain --ipfs http://localhost:5001 --node http://localhost:8020"
  },
  "devDependencies": {
    "@cardstack/cardpay-sdk": "1.0.46",
    "@graphprotocol/graph-cli": "0.20.0",
    "@graphprotocol/graph-ts": "graphprotocol/graph-ts#56adb62d9e4233c6fc6c38bc0519a8a566afdd9e",
    "@protofire/subgraph-toolkit": "0.1.2",
    "fs-extra": "^10.0.0",
    "node-fetch": "^2.6.1",
    "sync-fetch": "^0.3.1",
    "ts-node": "^10.9.1"
  },
  "private": true,
  "workspaces": {
    "nohoist": [
      "@graphprotocol/*",
      "@protofire/*"
    ]
  },
  "dependencies": {
    "glob": "^7.1.7"
  }
}
