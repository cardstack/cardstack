{{#cardstack-session as |session|}}
  {{#if model.error}}
    <div>Error loading user: {{model.error.detail}}</div>
  {{/if}}
  {{#if session.isAuthenticated }}
    {{#if model.avatarUrl}}
      <div>
        <img src={{model.avatarUrl}} width="200" alt="avatar">
      </div>
    {{/if}}
    <div>name: {{model.name}}</div>
    <div>email: {{model.email}}</div>
  {{/if}}
{{/cardstack-session}}
