{{#cardstack-session as |session|}}
  {{#if session.isAuthenticated }}
    <button {{action session.logout}}>Logout {{session.user.name}}</button>

    {{user-log}}
  {{else}}
    {{#mock-login as |login|}}
      <button {{action login "user1"}}>Login</button>
    {{/mock-login}}
  {{/if}}
{{/cardstack-session}}

{{cardstack-notification}}
