
<div class="label {{@card.name}}-label" data-test-link-field-viewer-label>
  {{or @card.csTitle @card.name}}
</div>
{{#if this.fieldValue}}
  <a
    class="value {{@card.name}}-value link-viewer"
    href={{this.fieldValue}}
    target="_blank"
    rel="noopener noreferrer"
    data-test-link-field-viewer-value
  >
    {{this.fieldValue}}
  </a>
{{/if}}