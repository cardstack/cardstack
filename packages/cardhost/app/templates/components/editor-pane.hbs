  <section class="cardhost-card-theme-editor {{if (eq this.dockLocation "bottom") "bottom-docked"}}">
    <div {{on "click" this.dockRight}}>right</div>
    <div {{on "click" this.dockBottom}}>bottom</div>
    <h3>Card Theme Editor</h3>
    <CodeEditor
      @code=".a {}"
      @language="css"
      @readOnly={{false}}
      @resizable={{true}}
    />
  </section>