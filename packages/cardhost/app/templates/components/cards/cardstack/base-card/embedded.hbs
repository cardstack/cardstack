{{!-- TODO move this into the @cardstack/base-card package when we support runtime js compilation --}}
<div
  class="embedded-card cardstack_base-card-embedded {{safe-css-string @card.name}}-embedded {{safe-css-string @card.adoptedFromName}}-embedded"
  data-test-embedded-card={{@card.name}}
  data-test-embedded-card-mode={{@mode}}>
  {{#each @card.embeddedFields key="nonce" as | field |}}
    <FieldRenderer @field={{field}} @mode={{@mode}}/>
  {{/each}}
</div>