<div class="editor-container">
  {{#if @title}}
    <div class="editor-container__title">{{@title}} ({{if this.readOnly "read-only" "editable"}})</div>
  {{/if}}
  <div {{did-insert this.renderEditor}}
    ...attributes
    data-test-code-block={{@code}}
    class="cardhost-monaco-container"
  >
  </div>
</div>