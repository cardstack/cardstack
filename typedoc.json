{
  "name": "cardstack",
  "$schema": "https://typedoc.org/schema.json",
  "out": "docs",
  "entryPointStrategy": "packages",
  "readme": "./README.md",
  "includeVersion": true,
  "plugin": ["typedoc-plugin-resolve-crossmodule-references"],
  "exclude": [
    "./packages/hub",
    "./packages/cardpay-cli",
    "./packages/boxel",
    "./packages/eslint-config",
    "./packages/demo-cards",
    "./packages/firebase-functions",
    "./packages/firebase-functions/review-feature/functions",
    "./packages/firebase-functions/push/functions",
    "./packages/firebase-functions/maintenance-status/functions",
    "./packages/firebase-functions/minimum-version/functions",
    "./packages/firebase-functions/check-version/functions",
    "./packages/web-client",
    "./packages/ember-shared/addon",
    "./packages/ember-shared/test-app",
    "./packages/compiled",
    "./packages/ssr-web",
    "./packages/safe-tools-subgraph",
    "./packages/test-support",
    "./packages/safe-tools-client",
    "./packages/core",
    "./packages/cardhost",
    "./packages/base-cards",
    "./packages/cardpay-subgraph",
    "./scripts"
  ],
  "sort": ["instance-first"]
}
