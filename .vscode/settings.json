{
  "eslint.validate": ["javascript", "typescript", "json"],
  "eslint.lintTask.enable": true,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.ignoreWords": [
    "ambabi",
    "fkey",
    "qqzv",
    "qrqzupcr",
    "r",
    "twtj",
    "wa",
    "walletorderholding"
  ],
  "cSpell.words": [
    "accum",
    "autotest",
    "corde",
    "createdb",
    "nakamoto",
    "rewardee",
    "satoshi",
    "skuinventories"
  ]
}
